{
  "name": "rork-kurdish-cuisine-cashier-system",
  "version": "1.0.0",
  "description": "Full-stack Expo + Supabase + Hono backend deployed on Netlify",
  "main": "expo-router/entry",
  "scripts": {
    "prestart": "bash scripts/startup-sync.sh",
    "start": "npx expo start --tunnel",
    "start:web": "npx expo start --web --tunnel",
    "start:mobile": "npx expo start --tunnel",
    "dev": "concurrently \"npm run start\" \"npm --prefix backend run start\"",
    "build:backend": "node scripts/install-backend.js && npm --prefix backend run build",
    "build:web": "npx expo export --platform web --output-dir dist --clear",
    "build:full": "npm run build:backend && npm run build:web",
    "build": "node scripts/automated-build.mjs",
    "doctor": "npx expo doctor",
    "prebuild": "npx expo prebuild --clean",
    "health:check": "node scripts/auto-health-check.mjs",
    "resync": "node scripts/resync-live.mjs",
    "sync:start": "bash scripts/startup-sync.sh"
  },
  "dependencies": {
    "@nkzw/create-context-hook": "1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "2.78.8",
    "@tanstack/react-query": "5.51.1",
    "@trpc/client": "11.7.1",
    "@trpc/react-query": "11.7.1",
    "@trpc/server": "11.7.1",
    "expo": "~52.0.17",
    "expo-asset": "~11.0.1",
    "expo-av": "~16.0.7",
    "expo-linear-gradient": "~14.0.1",
    "expo-router": "~3.6.18",
    "hono": "4.5.11",
    "lucide-react-native": "0.466.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "0.19.13",
    "superjson": "2.2.5",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@types/react-dom": "18.3.0",
    "@types/react": "18.3.4",
    "@types/react-native": "0.76.0",
    "concurrently": "8.2.2",
    "eslint": "9.31.0",
    "eslint-config-expo": "9.2.0",
    "typescript": "5.7.3"
  },
  "private": true
}
