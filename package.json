{
  "name": "rork-kurdish-cuisine-cashier-system",
  "version": "1.0.0",
  "description": "Full-stack Expo + Supabase + Hono backend deployed via Netlify",
  "main": "index.js",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir dist --clear --non-interactive",
    "build:backend": "npm --prefix backend run build",
    "build": "npm run build:backend && npm run build:web",
    "dev": "concurrently \"npm run start\" \"npm --prefix backend run start\""
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.86",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/react-query": "^5.90.6",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@ungap/structured-clone": "^1.3.0",
    "@hono/trpc-server": "^0.5.1",
    "dotenv": "^16.4.7",
    "expo": "~54.0.0",
    "expo-linear-gradient": "~15.0.0",
    "expo-router": "~6.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-web-browser": "~15.0.0",
    "hono": "^4.6.13",
    "lucide-react-native": "^0.552.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-safe-area-context": "4.14.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.19.13",
    "superjson": "^2.2.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "babel-preset-expo": "^10.0.0",
    "typescript": "^5.3.3",
    "@types/react": "~18.3.3",
    "@types/react-native": "~0.76.0",
    "@types/node": "^22.4.0"
  },
  "private": true
}