{
  "name": "rork-kurdish-cuisine-cashier-system",
  "version": "1.0.0",
  "description": "Full-stack Expo + Supabase + Hono backend deployed on Netlify",
  "main": "index.js",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:backend": "npm --prefix backend install && npm --prefix backend run build",
    "build": "npm run build:backend && npm run build:web",
    "dev": "concurrently \"npm run start\" \"npm --prefix backend run start\""
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.86",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query": "^5.90.6",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@ungap/structured-clone": "^1.3.0",
    "eslint": "9.31.0",
    "eslint-config-expo": "9.2.0",
    "expo": "^54.0.21",
    "expo-audio": "~1.0.14",
    "expo-av": "~16.0.7",
    "expo-router": "~6.0.14",
    "expo-status-bar": "~3.0.0",
    "expo-web-browser": "^15.0.8",
    "hono": "^4.6.13",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "superjson": "^2.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.73.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
