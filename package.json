{
  "name": "rork-kurdish-cuisine-cashier-system",
  "version": "1.0.0",
  "description": "Full-stack Rork Kurdish Cuisine Cashier System",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:backend": "npm --prefix backend install --legacy-peer-deps && npm --prefix backend run build",
    "build:web": "npx --yes expo export --platform web --output-dir dist --clear --non-interactive",
    "build": "npm run build:backend && npm run build:web"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.86",
    "@nkzw/create-context-hook": "^1.1.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
