{
  "name": "rork-kurdish-cuisine-cashier-system",
  "version": "1.0.0",
  "description": "Full-stack Expo + Supabase + Hono backend deployed on Netlify",
  "main": "index.js",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:backend": "npm --prefix backend install && npm --prefix backend run build",
    "build": "npm run build:backend && npm run build:web",
    "dev": "concurrently \"npm run start\" \"npm --prefix backend run start\""
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.86",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query": "^5.90.6",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@ungap/structured-clone": "^1.3.0",
    "eslint": "9.31.0",
    "eslint-config-expo": "9.2.0",
    "expo": "~51.0.0",
    "expo-av": "~14.0.7",
    "expo-router": "~3.5.14",
    "expo-status-bar": "~3.0.0",
    "expo-web-browser": "^12.0.0",
    "hono": "^4.6.13",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.10.0",
    "react-native-safe-area-context": "4.14.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.19.13",
    "superjson": "^2.2.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@types/react": "^18.2.42",
    "@types/react-native": "^0.73.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
